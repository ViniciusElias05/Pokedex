mon from './components/Pokemon.vue';\nexport default {\n  name: 'App',\n  data(){\n    return{\n      pokemons: [],\n      filteredPokemons: [],\n      busca: ''\n    }\n  },\n  created: function(){\n    axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=151&offset=0\").then(res =>{\n      console.log(\"Pegou a lista de Pokemon!\");\n      this.pokemons = res.data.results;\n      this.filteredPokemons = res.data.results;\n    })\n  },\n  components:{\n    Pokemon\n  },\n  methods:{\n    buscar: function (){\n      this.filteredPokemons = this.pokemons;\n      if(this.busca == '' || this.busca == ' '){\n        this.filteredPokemons = this.pokemons;\n      }else{\n       this.filteredPokemons = this.pokemons.filter(pokemon => pokemon.name==this.busca);\n      }\n    }\n  },\n  computed: {\n          /*resultadoBusca: function(){\n\n      if(this.busca == '' || this.busca == ' '){\n        return this.pokemons;\n      }else{\n        return this.pokemons.filter(pokemon => pokemon.name == this.busca);\n      }\n    }*/\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n#buscaBtn{\n  margin-top: 2%;\n}\n</style>\n"],"mappings":"AAiBA,OAAAA,KAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,gBAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA,WAAAA,CAAA;IACAP,KAAA,CAAAQ,GAAA,yDAAAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA;MACA,KAAAR,QAAA,GAAAM,GAAA,CAAAP,IAAA,CAAAU,OAAA;MACA,KAAAR,gBAAA,GAAAK,GAAA,CAAAP,IAAA,CAAAU,OAAA;IACA;EACA;EACAC,UAAA;IACAb;EACA;EACAc,OAAA;IACAC,MAAA,WAAAA,CAAA;MACA,KAAAX,gBAAA,QAAAD,QAAA;MACA,SAAAE,KAAA,eAAAA,KAAA;QACA,KAAAD,gBAAA,QAAAD,QAAA;MACA;QACA,KAAAC,gBAAA,QAAAD,QAAA,CAAAa,MAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAhB,IAAA,SAAAI,KAAA;MACA;IACA;EACA;EACAa,QAAA;IACA;;AAEA;AACA;AACA;AACA;AACA;EANA;AASA"},"metadata":{},"sourceType":"module","externalDependencies":[]}